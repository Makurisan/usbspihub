obj-m  := v-hub.o 
v-hub-objs := mpack.o vusb_mpack.o vusb_core.o vusb_udc.o vusb_spi.o  vusb_ep.o vusb_helper.o vusb_chrdev.o

KERNEL_DIR ?= ~/rpi/linux-5.10.17-v7

all:
	        make -C $(KERNEL_DIR) \
	                ARCH=arm CROSS_COMPILE=arm-linux-gnueabihf- \
					                M=$(PWD) modules
clean:
	        make -C $(KERNEL_DIR) \
			ARCH=arm CROSS_COMPILE=arm-linux-gnueabihf- \
						      M=$(PWD) clean
